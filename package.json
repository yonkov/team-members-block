{
	"name": "inpsyde-challenge",
	"version": "0.1.0",
	"description": "Example block written with ESNext standard and JSX support â€“ build step required.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"jest": {
		"preset": "@wordpress/jest-preset-default",
		"verbose": true,
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testMatch": [
			"**/__tests__/**/*.test.(tsx|jsx)"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"testEnvironment": "jsdom",
		"globals": {
			"inpsyde_challenge_script_params": {
				"rest_url": "/wp_json"
			}
		}
	},
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start",
		"packages-update": "wp-scripts packages-update",
		"test": "jest",
		"test:e2e": "wp-scripts test-e2e"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@types/node": "^17.0.8",
		"@types/react": "^17.0.38",
		"@types/wordpress__block-editor": "^6.0.4",
		"@types/wordpress__blocks": "^9.1.1",
		"@types/wordpress__components": "^14.0.9",
		"@wordpress/block-editor": "^8.0.13",
		"@wordpress/blocks": "^11.1.5",
		"@wordpress/components": "^19.2.0",
		"@wordpress/e2e-test-utils": "^5.4.5",
		"@wordpress/i18n": "^4.2.4",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.8",
		"@babel/preset-typescript": "^7.16.7",
		"@types/jest": "^27.4.0",
		"@wordpress/jest-preset-default": "^7.1.3",
		"@wordpress/scripts": "^19.2.2",
		"jest": "^27.1.3",
		"jest-environment-jsdom": "^27.0.6",
		"ts-jest": "^27.1.3"
	}
}
